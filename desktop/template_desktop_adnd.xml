<root>
  <!-- template for menu record list -->
  <template name="menu_refmanuals_list">
    <windowlist>
       <anchored >
          <top offset="5" />
          <left offset="5" />
          <right offset="-5"/>
          <bottom offset="-5" />
        </anchored>            
      <frame name="darkbox" offset="5,5,5,5" />
      <empty font="list-empty" textres="desktop_menu_refmanuals_empty" />
      <class>menu_record_item</class>
      <sortby><control>name</control></sortby>
    </windowlist>
  </template>

