<root>

  <template name="string_hypertext">
      <stringcontrol>
        <font>cta_hypertext</font>
        <lineoffset default="on" mergerule="replace">1</lineoffset>
        <cursor hover="arrow" />
        <readonly />
			  <multilinespacing>16</multilinespacing>
      </stringcontrol>
  </template>


  <template name="hyperlink_text">
      <string_hypertext name="hypertext">
        <script file="common/scripts/hyperlink_text.lua" />
      </string_hypertext>
  </template>

</root>